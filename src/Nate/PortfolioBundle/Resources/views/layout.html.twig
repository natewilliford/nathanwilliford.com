<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Nathan Williford's Portfolio{% endblock %}</title>
    <link href="http://fonts.googleapis.com/css?family=Quicksand:300,400,700" rel="stylesheet" type="text/css">
    <link href="{{ asset('bundles/nateportfolio/css/lightbox.css') }}" rel="stylesheet" />
    {% stylesheets '@NatePortfolioBundle/Resources/assets/css/main.scss' filter='compass' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {% javascripts '@NatePortfolioBundle/Resources/public/js/*' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

</head>
<body>

<div id="content">
    <div class="col1">
    {% block bio %}
    {% endblock %}
    </div>
    <div class="col2">
    {% block body %}{% endblock %}
    {% block fos_user_content %}{% endblock %}
    </div>
</div>
<div id="footer">
</div>
{% block downloads %}{% endblock %}
{% block javascripts %}{% endblock %}

<script type="text/javascript">
var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
</script>
<script type="text/javascript">
try {
var pageTracker = _gat._getTracker("UA-15031342-1");
pageTracker._trackPageview();
} catch(err) {}</script>

</body>
</html>
